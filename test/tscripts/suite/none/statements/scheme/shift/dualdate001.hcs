// none/statements/scheme/shift/dualdates.hcs   Test file

scheme J { name "Test Julian"; base julian; }

grammar JS {
    fields year month day;
    optional unshift;
    format dmyh "(day) |(month) |(year/unshift)";
}

scheme JA {
    name "Test Julian Annunciation";
    shift J, 1721507;    // j:dmy# 25 Mar 1
    grammar JS;
}

write date,JA:dmyh "12 2 1709/10";

/*[OUTPUT]
2345678
[OUTPUT]*/

