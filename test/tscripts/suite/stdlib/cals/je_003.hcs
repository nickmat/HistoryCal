// cals/je_003.hcs  Test Julian Easter calendar "je" with year ranges.

// Tests based on the following Julian Easter days
//     "18 Apr 1815", " 9 Apr 1816", "25 Mar 1817", "14 Apr 1818", " 6 Apr 1819",

writeln "";
writeln date.je:dmy "1815";  //j#( 18 Apr 1815 ..  8 Apr 1816 )
writeln date.je:dmy "1816";  //j#(  9 Apr 1816 .. 24 Mar 1817 )
writeln date.je:dmy "1817";  //j#( 25 Mar 1817 .. 13 Apr 1818 )
writeln date.je:dmy "1818";  //j#( 14 Apr 1818 ..  5 Apr 1819 )

