// range001.hcs Test range operator

let a = 3 .. 5;
let b = 5 .. 3;

let c = 3 .. 5 .. 8;
let d = 8 .. 3 .. 5;

let e = 3 .. 5 .. 4;
let f = 4 .. 3 .. 5;

let g = 3 .. 5 .. 2;
let h = 2 .. 3 .. 5;

write a +", "+ b +", "+ c +", "+ d +", "+ e +", "+ f +", "+ g +", "+ h;

clear;

