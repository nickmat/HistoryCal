// Modfied Julian Day and Rata Die
mark "MJD";

scheme "MJD" {
    name "Modfied Julian Day";
    shift "jdn", 2400001;
    grammar "d";
}

scheme "RD" {
    name "Rata Die";
    shift "jdn", 1721425;
    grammar "d";
}

grammar "JD" {
    inherit "d";
    format "d" {
        output "(Day).5";
    }
}

scheme "JD" {
    name "Julian Day";
    shift "jdn", 1;
    grammar "JD";
}